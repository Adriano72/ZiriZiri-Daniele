<Alloy>
	
	<Collection src = "events">
	<Window class="container" title = "Timeline">
		
		<TableView id = "timelineTable" dataCollection = "events" >
			<TableViewRow hasChild = "true" onClick = "mostraDettaglioEvento">
				<Label id = "rowIcon"/>
				<Label id = "name" text ="{name}"/>
				<Label id = "awesome" text="{awesome}"/>
				<Label id = "category" text ="{category}"/>
			</TableViewRow>
		</TableView>
		
	</Window>
	
</Alloy>